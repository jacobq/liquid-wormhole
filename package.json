{
  "name": "liquid-wormhole",
  "version": "2.1.5",
  "description": "Wormholes with the power of Liquid Fire",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/pzuraq/liquid-wormhole",
  "license": "MIT",
  "author": "Christopher Garrett <cmg@pzuraq.co>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^7.23.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-getowner-polyfill": "^3.0.2",
    "perf-primitives": "^0.0.6"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.22.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-github-pages": "0.2.2",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-markdown-compiler": "pzuraq/ember-cli-markdown-compiler#example-site-fixes",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-native-dom-helpers": "0.6.3",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.22.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.11.0",
    "eslint-plugin-ember": "^9.3.0",
    "eslint-plugin-node": "^11.1.0",
    "liquid-fire": "0.31.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.5.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "classic"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "liquid-fire"
    ]
  }
}
